#include <stdio.h>
#include <stdlib.h>
#include <math.h>

struct triangle
{
	int a;
	int b;
	int c;
};

typedef struct triangle triangle;
double calculate_area(triangle tr) {
    double s = (tr.a + tr.b + tr.c) / 2.0; // semi-perimeter
    return sqrt(s * (s - tr.a) * (s - tr.b) * (s - tr.c));
}

// Comparison function for sorting triangles by area
int compare(const void* a, const void* b) {
    triangle tr1 = *(triangle*)a;
    triangle tr2 = *(triangle*)b;
    
    double area1 = calculate_area(tr1);
    double area2 = calculate_area(tr2);
    
    if (area1 < area2) return -1; // tr1 comes before tr2
    if (area1 > area2) return 1;  // tr1 comes after tr2
    return 0; // areas are equal
}

void sort_by_area(triangle* tr, int n) {
	qsort(tr, n, sizeof(triangle), compare);
}
int main()
{
	int n;
	scanf("%d", &n);
	triangle *tr = malloc(n * sizeof(triangle));
	for (int i = 0; i < n; i++) {
		scanf("%d%d%d", &tr[i].a, &tr[i].b, &tr[i].c);
	}
	sort_by_area(tr, n);
	for (int i = 0; i < n; i++) {
		printf("%d %d %d\n", tr[i].a, tr[i].b, tr[i].c);
	}
	return 0;
}
